{
  "scenarios": {
    "get-post": {
      "requests": [
        "http://demo.blazemeter.com/?${somename}=${varname2}",
        {
          "url": "dummy",
          "label": "Disable.Me"
        },
        {
          "method": "POST",
          "multipart": true,
          "keepalive": true,
          "url": "http://demo.blazemeter.com/api/user",
          "label": "With body",
          "body": {
            "param1":"param1_val",
            "param2":"param2_val"
          }
        },
        {
          "method": "PUT",
          "url": "http://demo.blazemeter.com/api/user",
          "label": "With body from file",
          "body-file": "tests/jmx/http.jmx"
        }
      ],
      "properties": {
        "log_level.jmeter": "WARN"
      },
      "headers": {
        "Host": "blazemeter.com",
        "Connection": "keep-alive"
      },
      "store-cache": true,
      "store-cookie": true,
      "timeout": 1,
      "think-time": 0.5,
      "disable-listeners": true,
      "data-sources": [
        "tests/data/test1.csv",
        {
          "type": "csv",
          "path": "tests/data/test1.csv",
          "delimiter": ",",
          "quoted": false,
          "loop": true
        }
      ],
      "modifications": {
        "disable": ["Disable.Me"],
        "enable": "Disable.Me",
        "set-prop": {
          "Disable.Me>HTTPSampler.connect_timeout": "0",
          "Disable.Me>HTTPSampler.protocol": "https"
        }
      }
    }
  },
  "execution": {
    "concurrency": 5,
    "ramp-up": 15,
    "hold-for": 30,
    "iterations": 10,
    "scenario": "get-post"
  },
  "modules": {
    "jmeter": {
      "path": "build/jmeter/bin/jmeter"
    }
  }
}