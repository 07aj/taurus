node {
    stage("clean and checkout") {
        // clean if needed
        // cleanWs()   // requires workspace cleanup plugin to be installed
	checkout scm
        sh "pwd && ls -la"
    }
    
    stage("run test") {
        bzt "./examples/jmeter/stepping.yml -o settings.artifacts-dir=./artifacts -report"
    }

    stage("post-proc") {
	archiveArtifacts artifacts: 'artifacts/*.log'
	junit '**/xunit.xml'
    }
}
