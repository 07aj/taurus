node {
    stage("clean") {
        cleanWs()   // requires workspace cleanup plugin to be installed
        sh "pwd && ls -la"
    }
    
    stage("checkout") {
        checkout scm
        sh "pwd && ls -la"
    }

    stage("run test") {
        bzt "./examples/jmeter/stepping.yml -report"
    }

    stage("post-proc") {
        archiveArtifacts artifacts: '**/*.log'
        junit '**/xunit.xml'
    }
}
