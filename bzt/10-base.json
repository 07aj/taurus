{
  "provisioning": "local",
  "reporting": [
    "final_stats",
    "console"
  ],
  "modules": {
    "local": {
      "class": "bzt.modules.provisioning.Local"
    },
    "jmeter": {
      "class": "bzt.modules.jmeter.JMeterExecutor",
      "properties": {
        "jmeter.save.saveservice.autoflush": "true",
        "jmeterengine.force.system.exit": "true",
        "jmeter.save.saveservice.connect_time": "true",
        "summariser.name": ""
      },
      "path": "~/jmeter-taurus/bin/jmeter",
      "download_link": "http://apache.claz.org/jmeter/binaries/apache-jmeter-%s.zip",
      "plugins_download_link": "http://jmeter-plugins.org/files/JMeterPlugins-%s-1.2.1.zip",
      "version": "2.13"
    },
    "grinder": {
      "class": "bzt.modules.grinder.GrinderExecutor",
      "properties": {
        "grinder.useConsole": false
      },
      "path": "~/grinder-taurus"
    },
    "gatling": {
      "class": "bzt.modules.gatling.GatlingExecutor",
      "path": "~/gatling-taurus/bin/gatling.sh",
      "download_link": "https://repo1.maven.org/maven2/io/gatling/highcharts/gatling-charts-highcharts-bundle/%s/gatling-charts-highcharts-bundle-%s-bundle.zip",
      "version": "2.1.4"
    },
    "consolidator": {
      "class": "bzt.modules.aggregator.ConsolidatingAggregator",
      "percentiles": [
        0.0,
        50.0,
        90.0,
        95.0,
        99.0,
        99.9,
        100.0
      ]
    },
    "console": {
      "class": "bzt.modules.console.ConsoleStatusReporter",
      "disable": false
    },
    "loadosophia": {
      "class": "bzt.modules.loadosophia.Loadosophia"
    },
    "blazemeter": {
      "class": "bzt.modules.blazemeter.BlazeMeterUploader"
    },
    "final_stats": {
      "class": "bzt.modules.reporting.FinalStatus"
    },
    "fail-criteria": {
      "class": "bzt.modules.passfail.PassFailStatus"
    }
  },
  "settings": {
    "default-executor": "jmeter",
    "aggregator": "consolidator"
  },
  "aliases": {
    "report": {
      "reporting": [
        "blazemeter"
      ]
    }
  }
}